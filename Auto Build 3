local function ContinuousBuild()
    while true do
        local Hammer = Character:FindFirstChildOfClass("Tool")
        if not Hammer then
            break
        end
        for _, args in ipairs(plankConfigs) do
            BuildPlank(args)
        end
        wait(0.1)
    end
end

ContinuousBuild()
