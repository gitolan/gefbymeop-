MainFrame.Position = UDim2.new(0.5, -200, 0.5, -100)
MainFrame.Size = UDim2.new(0, 400, 0, 200)
MainFrame.Draggable = true
MainFrame.Active = true
MainFrame.Visible = false

-- ตั้งค่า OpenButton
OpenButton.Name = "OpenButton"
