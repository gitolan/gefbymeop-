        for _, miniGEF in pairs(GEFs:GetChildren()) do
            if miniGEF.Name == "Mini GEF" then
                local hurtbox = miniGEF:FindFirstChild("Hurtbox")
                if hurtbox and hurtbox:IsA("BasePart") then
                    hurtbox:Destroy()
                end
            end
        end
      end    
})
