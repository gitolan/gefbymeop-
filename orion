Tab:AddButton({
    Name = "Get TpTool",
    Callback = function()
              local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local IYMouse = LocalPlayer:GetMouse()
        local TpTool = Instance.new("Tool")
        TpTool.Name = "Tp Tool"
        TpTool.RequiresHandle = false
        TpTool.Parent = LocalPlayer.Backpack
        TpTool.Activated:Connect(function()
            local Char = LocalPlayer.Character or workspace:FindFirstChild(LocalPlayer.Name)
            local HRP = Char and Char:FindFirstChild("HumanoidRootPart")
            if not Char or not HRP then
                return warn("Failed to find HumanoidRootPart")
            end
            HRP.CFrame = CFrame.new(IYMouse.Hit.Position)
        end)
      end    
})
Tab:AddButton({
    Name = "No Gef Kill",
    Callback = function()
              local GEFs = game:GetService("Workspace").GEFs
        -- Loop through each "Mini GEF" in "GEFs" and destroy "hurtbox"
        for _, miniGEF in pairs(GEFs:GetChildren()) do
            if miniGEF.Name == "Mini GEF" then
                local hurtbox = miniGEF:FindFirstChild("Hurtbox")
                if hurtbox and hurtbox:IsA("BasePart") then
                    hurtbox:Destroy()
                end
            end
        end
      end    
})
Tab:AddButton({
    Name = "Hitbox gef",
    Callback = function()
              local miniGEFs = workspace.GEFs:GetChildren()

        for _, miniGEF in ipairs(miniGEFs) do
            if miniGEF:IsA("Model") and miniGEF:FindFirstChild("Hitbox") then
                miniGEF.Hitbox.Size = Vector3.new(30, 30, 30)
                miniGEF.Hitbox.Transparency = 0.8
            end
        end
      end    
})
Tab:AddButton({
    Name = "Delete all house and build",
    Callback = function()
              local function deleteBuildings()
            local buildings = workspace:FindFirstChild("Buildings")
            if buildings then
                buildings:Destroy()
            end
        end
        deleteBuildings()
      end    
})
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Variable to store the selected player
local selectedPlayer = nil

-- Function to bring the target player to your position
local function bringPlayerToMe(targetPlayer)
    if targetPlayer and targetPlayer.Character and player.Character then
        local targetCharacter = targetPlayer.Character
        local playerCharacter = player.Character

        local targetRoot = targetCharacter:FindFirstChild("HumanoidRootPart")
        local playerRoot = playerCharacter:FindFirstChild("HumanoidRootPart")

        if targetRoot and playerRoot then
            targetRoot.CFrame = playerRoot.CFrame
        end
    end
end

-- Initialize options
local options = {}

-- Placeholder for the tab or UI section you're working with
local Tab = {} -- Replace with your actual UI library's Tab object

-- Function to create or update the dropdown
local function createOrUpdateDropdown()
    if Tab.Dropdown then
        -- Remove the old dropdown if it exists
        Tab.Dropdown:Destroy()
    end

    -- Create a new dropdown
    Tab.Dropdown = Tab:AddDropdown({
        Name = "Player",
        Default = options[1] or "",
        Options = options,
        Callback = function(value)
            selectedPlayer = Players:FindFirstChild(value)
        end
    })
end

-- Initial setup of the dropdown
createOrUpdateDropdown()

-- Event to update dropdown when a player joins
Players.PlayerAdded:Connect(function(newPlayer)
    if newPlayer ~= player then
        table.insert(options, newPlayer.Name)
        createOrUpdateDropdown()
    end
end)

-- Event to update dropdown when a player leaves
Players.PlayerRemoving:Connect(function(leavingPlayer)
    for i, playerName in ipairs(options) do
        if playerName == leavingPlayer.Name then
            table.remove(options, i)
            break
        end
    end
    createOrUpdateDropdown()
end)

-- Setup button to bring the selected player
Tab:AddButton({
    Name = "Bring Player",
    Callback = function()
        if selectedPlayer then
            bringPlayerToMe(selectedPlayer)
        else
            print("No player selected")
        end
    end    
})

-- Initialize dropdown with existing players
for _, existingPlayer in ipairs(Players:GetPlayers()) do
    if existingPlayer ~= player then
        table.insert(options, existingPlayer.Name)
    end
end

-- Final update to populate the initial dropdown
createOrUpdateDropdown()

