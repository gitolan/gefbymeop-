        game.Players.LocalPlayer.RunData.Ammo.Bullets.Value = 5000
      end    
})
Tab:AddButton({
    Name = "Infmoney Visual",
    Callback = function()
              local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer
local originalMoneyValue = nil

if localPlayer then
    local moneyValue = localPlayer:FindFirstChild("Money")
    if moneyValue then
        if not originalMoneyValue then
            originalMoneyValue = moneyValue.Value
        end
        
        if moneyValue.Value == originalMoneyValue then
            moneyValue.Value = 100000
        else
            moneyValue.Value = originalMoneyValue
        end
    else
        warn("Value Money tidak ditemukan di LocalPlayer")
    end
else
    warn("LocalPlayer tidak ditemukan di Players service")
end
      end    
})
Tab:AddButton({
    Name = "AntiFling",
    Callback = function()
              local Services = setmetatable({}, {__index = function(Self, Index)
local NewService = game.GetService(game, Index)
if NewService then
Self[Index] = NewService
end
return NewService
end})

-- [ LocalPlayer ] --
local LocalPlayer = Services.Players.LocalPlayer

-- // Functions \\ --
local function PlayerAdded(Player)
   local Detected = false
   local Character;
   local PrimaryPart;

   local function CharacterAdded(NewCharacter)
       Character = NewCharacter
       repeat
           wait()
           PrimaryPart = NewCharacter:FindFirstChild("HumanoidRootPart")
       until PrimaryPart
       Detected = false
   end

   CharacterAdded(Player.Character or Player.CharacterAdded:Wait())
   Player.CharacterAdded:Connect(CharacterAdded)
   Services.RunService.Heartbeat:Connect(function()
       if (Character and Character:IsDescendantOf(workspace)) and (PrimaryPart and PrimaryPart:IsDescendantOf(Character)) then
           if PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 or PrimaryPart.AssemblyLinearVelocity.Magnitude > 100 then
               if Detected == false then
                   game.StarterGui:SetCore("ChatMakeSystemMessage", {
                       Text = "Fling Exploit detected, Player: " .. tostring(Player);
                       Color = Color3.fromRGB(255, 200, 0);
                   })
               end
               Detected = true
               for i,v in ipairs(Character:GetDescendants()) do
                   if v:IsA("BasePart") then
                       v.CanCollide = false
                       v.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
                       v.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
                       v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
                   end
               end
               PrimaryPart.CanCollide = false
               PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
               PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
               PrimaryPart.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
           end
       end
   end)
end

-- // Event Listeners \\ --
for i,v in ipairs(Services.Players:GetPlayers()) do
   if v ~= LocalPlayer then
       PlayerAdded(v)
   end
end
Services.Players.PlayerAdded:Connect(PlayerAdded)

local LastPosition = nil
Services.RunService.Heartbeat:Connect(function()
   pcall(function()
       local PrimaryPart = LocalPlayer.Character.PrimaryPart
       if PrimaryPart.AssemblyLinearVelocity.Magnitude > 250 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 250 then
           PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
           PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
           PrimaryPart.CFrame = LastPosition

           game.StarterGui:SetCore("ChatMakeSystemMessage", {
               Text = "You were fling. Neutralizing velocity.";
               Color = Color3.fromRGB(255, 0, 0);
           })
       elseif PrimaryPart.AssemblyLinearVelocity.Magnitude < 50 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 then
           LastPosition = PrimaryPart.CFrame
       end
       end)
       end)
      end    
})
local aimbotEnabled = false

Tab:AddToggle({
	Name = "Aimbot",
	Default = false,
	Callback = function(Value)
		aimbotEnabled = Value

		-- Services
		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")

		-- Player and Camera
		local player = Players.LocalPlayer
		local camera = workspace.CurrentCamera
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

		-- Function to get the nearest target
		local function getNearestTarget()
			local nearestTarget = nil
			local shortestDistance = math.huge

			for _, obj in pairs(workspace.GEFs:GetChildren()) do
				if obj:FindFirstChild("RootPart") then
					local distance = (humanoidRootPart.Position - obj.RootPart.Position).Magnitude
					if distance < shortestDistance then
						shortestDistance = distance
						nearestTarget = obj.RootPart
					end
				end
			end

			return nearestTarget
		end

		-- Function to aim at the nearest target
		local function aimAtTarget(target)
			if target then
				local targetPosition = target.Position

				-- Adjust the camera CFrame to aim at the target
				local desiredCFrame = CFrame.new(camera.CFrame.Position, targetPosition)
				camera.CFrame = desiredCFrame
			end
		end

		-- Update character and humanoidRootPart references when character respawns
		local function onCharacterAdded(newCharacter)
			character = newCharacter
			humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		end

		player.CharacterAdded:Connect(onCharacterAdded)

		-- Main loop to aim at the nearest target
		RunService.RenderStepped:Connect(function()
			if aimbotEnabled and character and humanoidRootPart then
				local nearestTarget = getNearestTarget()
				aimAtTarget(nearestTarget)
			end
		end)
	end
})

local players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local RunService = game:GetService("RunService")
local localPlayer = players.LocalPlayer
local options = {}

local options = {}
for _, player in ipairs(players:GetPlayers()) do
    if player ~= localPlayer then
        table.insert(options, player.Name)
    end
end

local selectedPlayer = nil
local flingEnabled = false
local fullbrightEnabled = false
local fullbrightValue = 3 -- Default value from the slider
local flingBodyAngularVelocity = nil
local initialPosition = nil
local initialCFrame = nil

local function updateLighting()
    if fullbrightEnabled then
        Lighting.Brightness = fullbrightValue
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = false
        Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    else
        Lighting.Brightness = 2
        Lighting.FogEnd = 10000
        Lighting.GlobalShadows = true
        Lighting.OutdoorAmbient = Color3.fromRGB(0, 0, 0)
    end
end

local function randomString(length)
    local charset = {}
    for i = 48, 57 do table.insert(charset, string.char(i)) end
    for i = 65, 90 do table.insert(charset, string.char(i)) end
    for i = 97, 122 do table.insert(charset, string.char(i)) end
    if length == nil then length = 10 end
    local result = ""
    for i = 1, length do
        result = result .. charset[math.random(1, #charset)]
    end
    return result
end

local function getRoot(character)
    return character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso")
end

local function setCharacterFlingProperties(character)
    for _, child in pairs(character:GetDescendants()) do
        if child:IsA("BasePart") then
            child.CustomPhysicalProperties = PhysicalProperties.new(math.huge, 0.3, 0.5)
        end
    end

    local root = getRoot(character)
    if root then
        flingBodyAngularVelocity = Instance.new("BodyAngularVelocity")
        flingBodyAngularVelocity.Name = randomString()
        flingBodyAngularVelocity.Parent = root
        flingBodyAngularVelocity.AngularVelocity = Vector3.new(0, 99999, 0)
        flingBodyAngularVelocity.MaxTorque = Vector3.new(0, math.huge, 0)
        flingBodyAngularVelocity.P = math.huge
    end

    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
            part.Massless = true
            part.Velocity = Vector3.new(0, 0, 0)
        end
    end
end

local function teleportToPlayer(player)
    if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") and player and player.Character then
        local root = getRoot(player.Character)
        if root then
            initialPosition = localPlayer.Character.HumanoidRootPart.Position
            initialCFrame = localPlayer.Character.HumanoidRootPart.CFrame
            localPlayer.Character.HumanoidRootPart.CFrame = root.CFrame
        end
    end
end

local function flingPlayer(player)
    if player and player.Character then
        setCharacterFlingProperties(player.Character)
    end
end

local function returnToInitialPosition()
    if initialPosition and initialCFrame then
        localPlayer.Character.HumanoidRootPart.CFrame = initialCFrame
    end
end

local function startFlinging()
    spawn(function()
        while flingEnabled do
            if selectedPlayer then
                teleportToPlayer(selectedPlayer)
                flingPlayer(selectedPlayer)
            end
            wait(0.1) -- Jeda waktu antar fling
        end
        returnToInitialPosition()
    end)
end

local function stopFlinging()
    if flingBodyAngularVelocity then
        flingBodyAngularVelocity:Destroy()
        flingBodyAngularVelocity = nil
    end
    returnToInitialPosition()
end

-- Initial call to set the default lighting settings
updateLighting()

-- Connect to the Heartbeat event to update lighting settings
RunService.Heartbeat:Connect(updateLighting)

-- Membuat Dropdown untuk pemain
for _, player in ipairs(players:GetPlayers()) do
    if player ~= localPlayer then
        table.insert(options, player.Name)
    end
end

local selectedPlayer = nil
local Dropdown

-- Function to create or update the dropdown
local function createOrUpdateDropdown()
    if Dropdown then
        -- Remove the old dropdown if it exists
        Dropdown:Destroy()
    end
    
    -- Create a new dropdown
    Dropdown = Tab:AddDropdown({
        Name = "PlayerDropdown",
        Default = options[1] or "",
        Options = options,
        Callback = function(Value)
            selectedPlayer = players:FindFirstChild(Value)
        end
    })
end

-- Initial creation of the dropdown
createOrUpdateDropdown()

-- Update dropdown when a player joins
players.PlayerAdded:Connect(function(player)
    if player ~= localPlayer then
        table.insert(options, player.Name)
        createOrUpdateDropdown()
    end
end)

-- Update dropdown when a player leaves
players.PlayerRemoving:Connect(function(leavingPlayer)
    for i, playerName in ipairs(options) do
        if playerName == leavingPlayer.Name then
            table.remove(options, i)
            break
        end
    end
    createOrUpdateDropdown()
end)

-- Membuat Toggle untuk fling
Tab:AddToggle({
    Name = "Fling Toggle",
    Default = false,
    Callback = function(Value)
        flingEnabled = Value
        if flingEnabled then
            startFlinging()
            NoclipToggle:Set(true)
        else
            stopFlinging()
        end
    end
})

Tab:AddSlider({
    Name = "Light Value",
    Min = 2,
    Max = 8,
    Default = 3,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "fullbright",
    Callback = function(Value)
        fullbrightValue = Value
        if fullbrightEnabled then
            updateLighting()
        end
    end
})

Tab:AddButton({
    Name = "Fullbright",
    Callback = function()
        fullbrightEnabled = not fullbrightEnabled
        updateLighting()
    end
})
Tab:AddButton({
    Name = "Refresh Dropdown",
    Callback = function()
        print("Refreshing dropdown")
        -- Update the options list with current players (excluding local player)
        options = {}
        for _, player in ipairs(players:GetPlayers()) do
            if player ~= localPlayer then
                table.insert(options, player.Name)
            end
        end
        createOrUpdateDropdown()
    end    
})

-- Initialize the library
OrionLib:Init()
