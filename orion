local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Gef Script OP", HidePremium = false, SaveConfig = true, ConfigFolder = "GefGui"})

local Tab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local teleportAndReturn = false
local initialPosition = nil

Tab:AddToggle({
	Name = "Teleport and Return",
	Default = false,
	Callback = function(Value)
		teleportAndReturn = Value
	end    
})

local function teleportAndReturnHandler(pickup)
    local player = game.Players.LocalPlayer
    local character = player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        if teleportAndReturn and not initialPosition then
            initialPosition = character.HumanoidRootPart.CFrame
        end
        
        character.HumanoidRootPart.CFrame = pickup.CFrame
        
        if teleportAndReturn and initialPosition then
            wait(1)  -- Adjust the wait time as needed
            character.HumanoidRootPart.CFrame = initialPosition
            initialPosition = nil  -- Reset the initial position after returning
        end
    end
end

Tab:AddButton({
    Name = "Ammo (Shotgun)",
    Callback = function()
        local pickup = game.Workspace.Pickups.Shells
        teleportAndReturnHandler(pickup)
    end    
})

Tab:AddButton({
    Name = "Ammo (Handgun)",
    Callback = function()
        local pickup = game.Workspace.Pickups.Bullets
        teleportAndReturnHandler(pickup)
    end    
})

